      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "nephosx-dev",
          "configurations": {
            "android": "1:714731543492:android:bd57746927fde862126291",
            "ios": "1:714731543492:ios:311eebe5dac53089126291",
