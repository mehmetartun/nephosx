{
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "celera-coach",
          "appId": "1:963212891501:android:8023be47281e82fd047664",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "celera-coach",
          "appId": "1:963212891501:ios:377fb29a2d1a3c22047664",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "celera-coach",
          "configurations": {
            "android": "1:963212891501:android:8023be47281e82fd047664",
            "ios": "1:963212891501:ios:377fb29a2d1a3c22047664",
            "web": "1:963212891501:web:2672d92465a1de1f047664"
          }
        }
      }
    }
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "disallowLegacyRuntimeConfig": true,
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin-allow-popups"
          }
        ]
      }
    ]
  }
}